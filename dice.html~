function update_counts() {
	var blue_num = 0;
	var yellow_num = 0;
	var green_num = 0;
	var total_num = 0;

	var all_dice = document.getElementsByClassName("die");
	for (i = 0; i<all_dice.length; i++) {
		if (all_dice[i].style.opacity != 0.0  && document.getElementById("blue" + all_dice[i].id.substring(3)).style.opacity != 0.0) {
			blue_num = blue_num + 1;
		}	
		if (all_dice[i].style.opacity != 0.0 && document.getElementById("yellow" + all_dice[i].id.substring(3)).style.opacity != 0.0) {
			yellow_num = yellow_num + 1;
		}	
		if (all_dice[i].style.opacity != 0.0 && document.getElementById("blue" + all_dice[i].id.substring(3)).style.opacity != 0.0 && document.getElementById("yellow" + all_dice[i].id.substring(3)).style.opacity != 0.0) {
			green_num = green_num + 1;
		}	
		if (all_dice[i].style.opacity != 0.0) {
			total_num = total_num + 1;
		}



	}
	document.getElementById("blue_count").innerHTML = blue_num;
	document.getElementById("yellow_count").innerHTML = yellow_num;
	document.getElementById("green_count").innerHTML = green_num;
	document.getElementById("total_count").innerHTML = total_num;	
}


function reset_buttons() {
	var ele = document.getElementsByName("roll");
	for(var i=0;i<ele.length;i++)
		 ele[i].checked = false;


	var ele = document.getElementsByName("rollb");
	for(var i=0;i<ele.length;i++)
		 ele[i].checked = false;


	var ele = document.getElementsByName("rollc");
	for(var i=0;i<ele.length;i++)
		 ele[i].checked = false;



}

function select(div_id) {
	var all_dice = document.getElementsByClassName("die");
	for (i = 0; i<all_dice.length; i++) {
		document.getElementById("blue" + all_dice[i].id.substring(3)).style.opacity = 0.0; 
	}
	
	if (div_id == 'none') {
		document.getElementById("event1").innerHTML = 'None';
															} else {

	document.getElementById("event1").innerHTML = id_to_string[div_id];

	for (i = 0; i<id_to_var[div_id].length; i++) {
		document.getElementById("blue" + id_to_var[div_id][i].id.substring(3)).style.opacity = 0.3; 
	}
															}
	update_counts();

}


function select_y(div_id) {
		var all_dice = document.getElementsByClassName("die");
	for (i = 0; i<all_dice.length; i++) {
		document.getElementById("yellow" + all_dice[i].id.substring(3)).style.opacity = 0.0; 
	}
	
	if (div_id == 'none') {
		document.getElementById("event2").innerHTML = 'None';
	} else {

	document.getElementById("event2").innerHTML = id_to_string[div_id];

	for (i = 0; i<id_to_var[div_id].length; i++) {
		document.getElementById("yellow" + id_to_var[div_id][i].id.substring(3)).style.opacity = 0.5; 
	}
															}
	update_counts();
}


function select_c(div_id) {
	var all_dice = document.getElementsByClassName("die");
	for (i = 0; i<all_dice.length; i++) {
		all_dice[i].style.opacity = 1.0;
	}
	
	if (div_id == 'none') {
		document.getElementById("condition").innerHTML = 'None';
															} else {

	document.getElementById("condition").innerHTML = id_to_string[div_id];
	var all_dice = document.getElementsByClassName("die");
	for (i = 0; i<all_dice.length; i++) {
		all_dice[i].style.opacity = 0.0;
	}
	
	for (i = 0; i<id_to_var[div_id].length; i++) {
		id_to_var[div_id][i].style.opacity = 1.0;
	}
}
	update_counts();
}

	var all_dice_outer = document.getElementsByClassName("die");
	for (i = 0; i<all_dice_outer.length; i++) {
		all_dice_outer[i].style.opacity = 1.0;
	}
	

function given(div_id) {
	var all_dice = document.getElementsByClassName("die");
	for (i = 0; i<all_dice.length; i++) {
		all_dice[i].style.opacity = 0.01;
		all_dice[i].style.background = "white";
	}
	
	for (i = 0; i<id_to_var[div_id].length; i++) {
		id_to_var[div_id][i].style.opacity = 1.0;
		//id_to_var[div_id][i].style.background = "blue";
	}

}


