var last_seen = 0;

function show_next_panel() {
	last_seen++;
	document.getElementById("image_" + last_seen).style.opacity = 1;
	if (last_seen > 1) {
	document.getElementById("possibility_" + last_seen).style.opacity = 1;

	} else {
		poss.style.opacity = 1;
		prior.style.opacity = 0;
		lik.style.opacity = 0;
		post.style.opacity = 0;
	}	
}



