	function random_choice_from_list(this_array) {
		return this_array[Math.floor(Math.random() * this_array.length)];
	}

	var current_extension = [];
	var current_learner_image = 0;
	var possibilities = [];
	var poss_pairs = [];
	var current_word = "";
	var chosen_image = "NONE";
	
	
	function FireEvent( ElementId, EventName )
	{
    		if( document.getElementById(ElementId) != null )    
    		{   
        		if( document.getElementById( ElementId ).fireEvent ) 
        		{
            			document.getElementById( ElementId ).fireEvent( 'on' + EventName );     
        		}
        		else 
        		{   
            			var evObj = document.createEvent( 'Events' );
            			evObj.initEvent( EventName, true, false );
            			document.getElementById( ElementId ).dispatchEvent( evObj );
        		}
    		}
	}


var global_demo_name = 0;

	function demo(demo_id) {
	

		
		
		global_demo_name = demo_id;

		chosen_image = "NONE";
     		for (x = 1; x < 7; x++) {
     			document.getElementById("head_" + x).style.opacity = 0.0;
     		}
	

		for (v = 1; v < 6; v++) {
     			for (vwx = 1; vwx < 7; vwx++) {
				document.getElementById("math_" + vwx + "_" + v).style.opacity = 0.0;
		
     			}
     		}     


		current_extension = dict[demo_id];

				var animal_classes = ["wolf", "rhino", "horse", "roadrunner", "owl", "goldfinch", "raven", "clownfish", "shark", "tuna", "snake", "tortoise", "frog", "spider", "ant", "beetle", "grasshopper", "octopus", "clam"];

     				for (ij = 1; ij < animal_classes.length + 1; ij++) {
					animal_class = document.getElementsByClassName(animal_classes[ij-1]);
     					for (jk = 1; jk < animal_class.length + 1; jk++) {
     						animal_class[jk-1].onclick = function () {};
						animal_class[jk-1].onmouseover = function () {};
     						animal_class[jk-1].onmouseout = function () {};
     						animal_class[jk-1].style.opacity = 1.0;
     						
					}	


				}
		
		if (madeup.includes(demo_id)) {

		} else {
			FireEvent(demo_id + "_p", "mouseover");
		}
     		
     


		if (madeup.includes(demo_id)) {

		} else {





			for (lm = 1; lm < current_extension.length + 1; lm++) {
				this_name = current_extension[lm-1].slice();
				document.getElementById(current_extension[lm-1].slice(0,-4)).onclick = function () {chosen_image = this.id + ".jpg"; show_image_learner(); }
				document.getElementById(current_extension[lm-1].slice(0,-4)).onmouseover = function () {this.style.opacity = 0.7;}	
				document.getElementById(current_extension[lm-1].slice(0,-4)).onmouseout = function () {this.style.opacity = 1.0;} 
				document.getElementById(current_extension[lm-1].slice(0,-4)).style.opacity = 1.0;
     
     			}
		}


		for (i=1; i<6; i++) {
			document.getElementById("learner_image_" + i).src = "blank_space.png";
     			document.getElementById("learner_word_" + i).innerHTML = "";
     			for (j=1; j<6; j++) {
     				document.getElementById("learner_pos_" + i + "_" + j).innerHTML = "";
				document.getElementById("learner_pos_" + i + "_" + j+ "n").innerHTML = "";	

     				document.getElementById("learner_pos_" + i + "_" + j).onmouseover = "";
     				document.getElementById("learner_pos_" + i + "_" + j).onmouseout = "";
     			}
     		}


		current_learner_image = 1;
		current_word = demo_id.charAt(0).toUpperCase() + demo_id.substr(1);
	
     			
		
     		document.getElementById("word_display").innerHTML = '"' + current_word + '"';
     
     	}

	function compare_poss(a,b) {
		if (a[1] < b[1]) {
     			return 1;
     		} 
     		if (a[1] > b[1]) {
			return -1;
		}
		return 0;


	}
     
var norm = 1;
var show_math = true;
var teacher = false;

	function show_image_learner() {
     		
			
	

		
		norm = 0
		
		if (chosen_image != "NONE") {
			this_image = chosen_image;
		} else {
			this_image = current_extension[current_learner_image - 1]; 
		}

		document.getElementById("learner_image_" + current_learner_image).src = this_image;
     
	     document.getElementById("learner_word_" + current_learner_image).innerHTML = '"' + current_word + '"';


		if (show_math) {
		if (current_learner_image == 1) {
			possibilities = options[this_image];
			
			for (u = 1; u < 6; u++) {
				document.getElementById("math_1_" + u).innerHTML = possibilities[u-1];
     			}

			poss_pairs = [];		
		
	
			for (j = 1; j < 6; j++) {
				post = prior[possibilities[j-1]] * 1.0 / count[possibilities[j-1]];
				poss_pairs.push([possibilities[j-1], post]);
				norm = norm + post;



     			}


     		} else {
	
     
     				for (j = 1; j < 6; j++) {
     if (options[this_image].includes(poss_pairs[j-1][0])) {
					poss_pairs[j-1][1] = poss_pairs[j-1][1] * 1.0; // 
					norm = norm + poss_pairs[j-1][1];



			     	} else {
					poss_pairs[j-1][1] = 0;
				}			

   
     			}
			

     		}


     		var poss_pairs_copy = poss_pairs.slice();
     		poss_pairs.sort(compare_poss);

     		for (j = 1; j < 6; j++) {
			document.getElementById("learner_pos_" + current_learner_image + "_" + j).innerHTML = poss_pairs[j-1][0];


			document.getElementById("learner_pos_" + current_learner_image + "_" + j + "n").innerHTML = (poss_pairs[j-1][1] / norm).toFixed(2);

			document.getElementById("learner_pos_" + current_learner_image + "_" + j).onmouseover = document.getElementById(options_rev[poss_pairs[j-1][0]].replace("_", "") + "_p").onmouseover; 
     			document.getElementById("learner_pos_" + current_learner_image + "_" + j).onmouseout = document.getElementById(options_rev[poss_pairs[j-1][0]].replace("_", "") + "_p").onmouseout; 


		}     

		for (v = 1; v < 6; v++) {
     			for (vwx = 1; vwx < 7; vwx++) {
				document.getElementById("math_" + vwx + "_" + v).style.color = "black";
		
     			}




		}     



     		function put_in_order() {

     			var new_poss_pairs = [];

     			for (i = 1; i<6; i++) {
				new_poss_pairs.push([i, poss_pairs_copy[i-1][1]]); 
     			}

     			new_poss_pairs = new_poss_pairs.sort(function(a,b){return b[1] - a[1];});


     			function process_index(ind, image_ind) {
     				return function() {
     					var math_index = new_poss_pairs[ind-1][0];

     					document.getElementById("math_1_" + math_index).style.color = "red";
					document.getElementById("math_6_" + math_index).style.color = "red";
					function vanish_table() {
						return function() {
     							
							document.getElementById("math_1_" + math_index).style.color = "silver";
     							document.getElementById("math_2_" + math_index).style.color = "silver";
     							document.getElementById("math_3_" + math_index).style.color = "silver";
     							document.getElementById("math_4_" + math_index).style.color = "silver";
     							document.getElementById("math_5_" + math_index).style.color = "silver";
     							document.getElementById("math_6_" + math_index).style.color = "silver";
							document.getElementById("learner_pos_" + image_ind + "_" + ind).style.opacity = 1.0;    
							if (ind == 1) {
     								for (uv = 1; uv < 6; uv++) {
     									document.getElementById("learner_pos_" + uv + "_" + ind).style.fontWeight = "normal";
     								}
     								document.getElementById("learner_pos_" + image_ind + "_" + ind).style.fontWeight = "bold";
     								
     							}

							document.getElementById("learner_pos_" + image_ind + "_" + ind + "n").style.opacity = 1.0;


     						}
					}
     					
     					setTimeout(vanish_table(), 500);
     				

     				}

     			}

     			for (m = 1; m < 6; m++) {
				document.getElementById("learner_pos_" + current_learner_image + "_" + m).style.opacity = 0.0;    
				document.getElementById("learner_pos_" + current_learner_image + "_" + m + "n").style.opacity = 0.0;


     			}

			function reveal_column(col_no) {
				return function() {

     					document.getElementById("head_" + col_no).style.opacity = 1.0;
					for (xy = 1; xy < 6; xy++) {
						document.getElementById("math_" + col_no + "_" + xy).style.opacity = 1.0;
     					}
     				}
	
     			}

     			
			setTimeout(process_index(1, current_learner_image), 1000);
     			setTimeout(process_index(2, current_learner_image), 2000);
     			setTimeout(process_index(3, current_learner_image), 3000);
     			setTimeout(process_index(4, current_learner_image), 4000);
     			setTimeout(process_index(5, current_learner_image), 5000);

		
     		} //end put_in_order


		put_in_order();

		current_learner_image++;

		} 
		
		
		

	}	



function show_image_learner_2() {
		for (x = 1; x < 7; x++) {
     			document.getElementById("head_" + x).style.opacity = 0.0;
     		}
	

		for (v = 1; v < 6; v++) {
     			for (vwx = 1; vwx < 7; vwx++) {
				document.getElementById("math_" + vwx + "_" + v).style.opacity = 0.0;
		
     			}
     		}     

     			
     			for (m = 1; m < 6; m++) {
				document.getElementById("learner_pos_" + current_learner_image + "_" + m).style.opacity = 0.0;    
				document.getElementById("learner_pos_" + current_learner_image + "_" + m + "n").style.opacity = 0.0;


     			}

			
		
		
		norm = 0
		
		if (chosen_image != "NONE") {
			this_image = chosen_image;
		} else {
			this_image = current_extension[current_learner_image - 1]; 
		}

		document.getElementById("learner_image_" + current_learner_image).src = this_image;
     
	     document.getElementById("learner_word_" + current_learner_image).innerHTML = '"' + current_word + '"';


		if (show_math) {
		if (current_learner_image == 1) {
			possibilities = options[this_image];
			
			for (u = 1; u < 6; u++) {
				document.getElementById("math_1_" + u).innerHTML = possibilities[u-1];
     			}

			poss_pairs = [];		
		
	
			for (j = 1; j < 6; j++) {
				post = prior[possibilities[j-1]] * 1.0 / count[possibilities[j-1]];
				poss_pairs.push([possibilities[j-1], post]);
				norm = norm + post;

				document.getElementById("math_2_" + j).innerHTML = prior[possibilities[j-1]];
				document.getElementById("math_3_" + j).innerHTML = count[possibilities[j-1]];
     				document.getElementById("math_4_" + j).innerHTML = (1.0/ count[possibilities[j-1]]).toPrecision(2);


     				document.getElementById("math_5_" + j).innerHTML = post.toPrecision(2);
     			}

     			for (j = 1; j < 6; j++) {
				document.getElementById("math_6_" + j).innerHTML = (poss_pairs[j-1][1]*1.0/norm).toPrecision(2);
			
     			}

     		} else {
	
     				for (u = 1; u < 6; u++) {
                                	document.getElementById("math_1_" + u).innerHTML = poss_pairs[u-1][0];
                        	}     		
     
     				for (j = 1; j < 6; j++) {
     if (options[this_image].includes(poss_pairs[j-1][0])) {
					poss_pairs[j-1][1] = poss_pairs[j-1][1] * 1.0 / count[poss_pairs[j-1][0]];
					norm = norm + poss_pairs[j-1][1];



			     	} else {
					poss_pairs[j-1][1] = 0;
				}			

				document.getElementById("math_2_" + j).innerHTML = prior[poss_pairs[j-1][0]];
				document.getElementById("math_3_" + j).innerHTML = count[poss_pairs[j-1][0]];
     				document.getElementById("math_4_" + j).innerHTML = (poss_pairs[j-1][1]/prior[poss_pairs[j-1][0]]).toPrecision(2);


     				document.getElementById("math_5_" + j).innerHTML = poss_pairs[j-1][1].toPrecision(2);
   
     			}
			
     			for (j = 1; j < 6; j++) {
				document.getElementById("math_6_" + j).innerHTML = (poss_pairs[j-1][1]/norm).toPrecision(2);     
			}

     		}


     		var poss_pairs_copy = poss_pairs.slice();
     		poss_pairs.sort(compare_poss);

     		for (j = 1; j < 6; j++) {
			document.getElementById("learner_pos_" + current_learner_image + "_" + j).innerHTML = poss_pairs[j-1][0];


			document.getElementById("learner_pos_" + current_learner_image + "_" + j + "n").innerHTML = (poss_pairs[j-1][1] / norm).toFixed(2);

			document.getElementById("learner_pos_" + current_learner_image + "_" + j).onmouseover = document.getElementById(options_rev[poss_pairs[j-1][0]].replace("_", "") + "_p").onmouseover; 
     			document.getElementById("learner_pos_" + current_learner_image + "_" + j).onmouseout = document.getElementById(options_rev[poss_pairs[j-1][0]].replace("_", "") + "_p").onmouseout; 


		}     

		for (v = 1; v < 6; v++) {
     			for (vwx = 1; vwx < 7; vwx++) {
				document.getElementById("math_" + vwx + "_" + v).style.color = "black";
		
     			}




		}     



     		function put_in_order() {

     			var new_poss_pairs = [];

     			for (i = 1; i<6; i++) {
				new_poss_pairs.push([i, poss_pairs_copy[i-1][1]]); 
     			}

     			new_poss_pairs = new_poss_pairs.sort(function(a,b){return b[1] - a[1];});


     			function process_index(ind, image_ind) {
     				return function() {
     					var math_index = new_poss_pairs[ind-1][0];

     					document.getElementById("math_1_" + math_index).style.color = "red";
					document.getElementById("math_6_" + math_index).style.color = "red";
					function vanish_table() {
						return function() {
     							
							document.getElementById("math_1_" + math_index).style.color = "silver";
     							document.getElementById("math_2_" + math_index).style.color = "silver";
     							document.getElementById("math_3_" + math_index).style.color = "silver";
     							document.getElementById("math_4_" + math_index).style.color = "silver";
     							document.getElementById("math_5_" + math_index).style.color = "silver";
     							document.getElementById("math_6_" + math_index).style.color = "silver";
							document.getElementById("learner_pos_" + image_ind + "_" + ind).style.opacity = 1.0;    
							if (ind == 1) {
     								for (uv = 1; uv < 6; uv++) {
     									document.getElementById("learner_pos_" + uv + "_" + ind).style.fontWeight = "normal";
     								}
     								document.getElementById("learner_pos_" + image_ind + "_" + ind).style.fontWeight = "bold";
     								
     							}

							document.getElementById("learner_pos_" + image_ind + "_" + ind + "n").style.opacity = 1.0;


     						}
					}
     					
     					setTimeout(vanish_table(), 500);
     				

     				}

     			}

     			for (m = 1; m < 6; m++) {
				document.getElementById("learner_pos_" + current_learner_image + "_" + m).style.opacity = 0.0;    
				document.getElementById("learner_pos_" + current_learner_image + "_" + m + "n").style.opacity = 0.0;


     			}

			function reveal_column(col_no) {
				return function() {

     					document.getElementById("head_" + col_no).style.opacity = 1.0;
					for (xy = 1; xy < 6; xy++) {
						document.getElementById("math_" + col_no + "_" + xy).style.opacity = 1.0;
     					}
     				}
	
     			}

			setTimeout(reveal_column(1), 1000);
     			setTimeout(reveal_column(2), 2000);
     			setTimeout(reveal_column(3), 3000);
     			setTimeout(reveal_column(4), 4000);
     			setTimeout(reveal_column(5), 5000);
     			setTimeout(reveal_column(6), 6000);
     			
			setTimeout(process_index(1, current_learner_image), 7000);
     			setTimeout(process_index(2, current_learner_image), 8000);
     			setTimeout(process_index(3, current_learner_image), 9000);
     			setTimeout(process_index(4, current_learner_image), 10000);
     			setTimeout(process_index(5, current_learner_image), 11000);

		
     		} //end put_in_order





		} 
		
		
		

	}	


     function blank() {
		for (var this_class = 0; this_class < classes.length; this_class++) {
    			var class_set = document.getElementsByClassName(classes[this_class]);
    			for (var i = 0; i < class_set.length; i++) {
    				class_set[i].style.opacity = 0.1;      
    			}
    		} 
     }

function light(some_elt) {
	document.getElementById(some_elt).style.opacity = 1;
}

function highlight(some_class) {


//	for (var this_class = 0; this_class < classes.length; this_class++) {
//		var class_set = document.getElementsByClassName(classes[this_class]);
//		for (var i = 0; i < class_set.length; i++) {
//			class_set[i].style.opacity = 0.1;	
//		}		
//	}
	
	var class_set = document.getElementsByClassName(some_class);
	for (var i = 0; i < class_set.length; i++) {
		class_set[i].style.opacity = 1;
	}
}

var classes = ["wolf", "rhino", "horse", "roadrunner", "owl", "goldfinch", "raven", "clownfish", "shark", "tuna", "snake", "tortoise", "frog", "spider", "ant", "beetle", "grasshopper", "octopus", "clam"];

function unhighlight() {
	for (var this_class = 0; this_class < classes.length; this_class++) {
		var class_set = document.getElementsByClassName(classes[this_class]);
		for (var i = 0; i < class_set.length; i++) {
			class_set[i].style.opacity = 1;	
		}		
	}


		
		if (madeup.includes(global_demo_name)) {

		} else {
			FireEvent(global_demo_name + "_p", "mouseover");
		}

}

     function clear_all() {

    		chosen_image = "NONE";
     		global_demo_name = "NONE";
		current_word = "";
     		current_learner_image = 0;

		document.getElementById("word_display").innerHTML = "&nbsp";
     		
		for (x = 1; x < 7; x++) {
     			document.getElementById("head_" + x).style.opacity = 0.0;
     		}
	

		for (v = 1; v < 6; v++) {
     			for (vwx = 1; vwx < 7; vwx++) {
				document.getElementById("math_" + vwx + "_" + v).style.opacity = 0.0;
		
     			}
     		}     


				var animal_classes = ["wolf", "rhino", "horse", "roadrunner", "owl", "goldfinch", "raven", "clownfish", "shark", "tuna", "snake", "tortoise", "frog", "spider", "ant", "beetle", "grasshopper", "octopus", "clam"];

     				for (ij = 1; ij < animal_classes.length + 1; ij++) {
					animal_class = document.getElementsByClassName(animal_classes[ij-1]);
     					for (jk = 1; jk < animal_class.length + 1; jk++) {
     						animal_class[jk-1].onclick = function () {};
						animal_class[jk-1].onmouseover = function () {};
     						animal_class[jk-1].onmouseout = function () {};
     						animal_class[jk-1].style.opacity = 1.0;
     						
					}	


				}

     }



