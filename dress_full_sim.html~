<script>

function allowDrop(ev) {
	ev.preventDefault();
}

function drag(ev) {
	ev.dataTransfer.setData("text", ev.target.id);
}

function drop(ev) {
	ev.preventDefault();
	var data = ev.dataTransfer.getData("text");
	ev.target.appendChild(document.getElementById(data));
}



var sim_started = 0;
var blue_counter = 1;
var gold_counter = 1;
var total_blue = 0;
var total_gold = 0;

document.getElementById("blue_catcher").innerHTML = "";
document.getElementById("gold_catcher").innerHTML = "";
document.getElementById("seconds_b").innerHTML = "";
document.getElementById("seconds_y").innerHTML = "";

var posterior_white_gold = 0;
var posterior_black_blue = 0;


function choose_bb() {
	if (posterior_black_blue >= posterior_white_gold) {
		right_choice.style.opacity = 1.0;
		right_choice.style.zIndex = 100;
		document.getElementById("right_color_right_choice").innerHTML = "black and blue";
		document.getElementById("right_color_2_right_choice").innerHTML = "black and blue";	
		document.getElementById("wrong_color_right_choice").innerHTML = "white and gold";	
	} else {
		wrong_choice.style.opacity = 1.0;
		wrong_choice.style.zIndex = 100;
		document.getElementById("right_color_wrong_choice").innerHTML = "black and blue";
		document.getElementById("wrong_color_wrong_choice").innerHTML = "white and gold";	
		
	}
}


function choose_wg() {
	if (posterior_white_gold >= posterior_black_blue) {
		right_choice.style.opacity = 1.0;
		right_choice.style.zIndex = 100;
		document.getElementById("right_color_right_choice").innerHTML = "white and gold";
		document.getElementById("right_color_2_right_choice").innerHTML = "white and gold";	
		document.getElementById("wrong_color_right_choice").innerHTML = "black and blue";	
	} else {
		wrong_choice.style.opacity = 1.0;
		wrong_choice.style.zIndex = 100;
		document.getElementById("right_color_wrong_choice").innerHTML = "white and gold";
		document.getElementById("wrong_color_wrong_choice").innerHTML = "black and blue";	
	
	}
}



function plug_in_bbef() {
	bbef_button.style.opacity = 0.0;
	bbef_button.style.zIndex = -100;
	bbef_eq_2.style.opacity = 1.0;

	var interv = setInterval(reveal_rhs_2_bb, 50);
	opacity_rhs_2_bb = 0;

	function reveal_rhs_2_bb() {
		if (opacity_rhs_2_bb == 30) {
			clearInterval(interv);
			bbef_eq_3.style.opacity = 1.0;
			var interv2 = setInterval(reveal_rhs_3_bb, 50);
			opacity_rhs_3_bb = 0;
		} else {
			opacity_rhs_2_bb++;
			bbef_rhs_2.style.opacity = opacity_rhs_2_bb * 1.0 / 30;
		}
	}	

	function reveal_rhs_3_bb() {
		if (opacity_rhs_3_bb == 30) {
			clearInterval(interv2);
		} else {
			opacity_rhs_3_bb++;
			bbef_rhs_3.style.opacity = opacity_rhs_3_bb * 1.0 / 30;
		}
	}

}


function plug_in_wgef() {
	wgef_button.style.opacity = 0.0;
	wgef_button.style.zIndex = -100;
	wgef_eq_2.style.opacity = 1.0;

	var interv = setInterval(reveal_rhs_2_wg, 50);
	opacity_rhs_2_wg = 0;

	function reveal_rhs_2_wg() {
		if (opacity_rhs_2_wg == 30) {
			clearInterval(interv);
			wgef_eq_3.style.opacity = 1.0;
			var interv2 = setInterval(reveal_rhs_3_wg, 50);
			opacity_rhs_3_wg = 0;
		} else {
			opacity_rhs_2_wg++;
			wgef_rhs_2.style.opacity = opacity_rhs_2_wg * 1.0 / 30;
		}
	}	

	function reveal_rhs_3_wg() {
		if (opacity_rhs_3_wg == 30) {
			clearInterval(interv2);
		} else {
			opacity_rhs_3_wg++;
			wgef_rhs_3.style.opacity = opacity_rhs_3_wg * 1.0 / 30;
		}
	}

}


function plug_in() {
	document.getElementById("bbef_num_2").innerHTML = (Math.round(((seconds_y * 1.0 / (seconds_b + seconds_y)) + 0.00001) * 100) / 100)  + " * " + (Math.round(((total_blue * 1.0 / (total_blue + total_gold)) + 0.00001) * 100) / 100); 
	document.getElementById("bbef_denom_2").innerHTML = Math.round(((total_blue * seconds_y * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))  + total_gold * seconds_b * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))     ) + 0.00001) * 100) / 100; 
	posterior_black_blue = Math.round((((((seconds_y * 1.0 / (seconds_b + seconds_y)))  *  ((total_blue * 1.0 / (total_blue + total_gold)))) / ((total_blue * seconds_y * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))  + total_gold * seconds_b * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))     ))) + 0.00001) * 100) / 100;
	document.getElementById("bbef_rhs_3").innerHTML = posterior_black_blue;

	document.getElementById("wgef_num_2").innerHTML = (Math.round(((seconds_b * 1.0 / (seconds_b + seconds_y)) + 0.00001) * 100) / 100)  + " * " + (Math.round(((total_gold * 1.0 / (total_blue + total_gold)) + 0.00001) * 100) / 100); 
	document.getElementById("wgef_denom_2").innerHTML = Math.round(((total_blue * seconds_y * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))  + total_gold * seconds_b * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))     ) + 0.00001) * 100) / 100; 
	posterior_white_gold = Math.round((((((seconds_b * 1.0 / (seconds_b + seconds_y)))  *  ((total_gold * 1.0 / (total_blue + total_gold)))) / ((total_blue * seconds_y * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))  + total_gold * seconds_b * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))     ))) + 0.00001) * 100) / 100;
	document.getElementById("wgef_rhs_3").innerHTML = posterior_white_gold;




}

