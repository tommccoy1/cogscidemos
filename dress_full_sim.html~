function check_blackblue() {
	if (((z_bb.contains(z_drag_bb) && z_wg.contains(z_drag_wg)) || (z_bb.contains(z_drag_wg) && z_wg.contains(z_drag_bb)))  &&  ((likelihood_bb.contains(likelihood_drag_bb) && prior_bb.contains(prior_drag_bb)) || (likelihood_bb.contains(prior_drag_bb) && prior_bb.contains(likelihood_drag_bb)))      &&  ((likelihood_wg.contains(likelihood_drag_wg) && prior_wg.contains(prior_drag_wg)) || (likelihood_wg.contains(prior_drag_wg) && prior_wg.contains(likelihood_drag_wg)))      ) {
		next4.disabled = false;	
	}	
}


function check_p_wg() {
	a2_check.style.opacity = 1.0;
	a2_check.style.zIndex = 100;

	if (Math.round((parseFloat(p_wg_form.elements[0].value) + 0.00001) * 100) / 100 == Math.round(((total_gold * 1.0 / (total_blue + total_gold)) + 0.00001) * 100) / 100) {
		document.getElementById("check_2_box").innerHTML = "Correct!";
	} else {
		document.getElementById("check_2_box").innerHTML = "Incorrect"
	}
}


function check_p_bb() {
	a1_check.style.opacity = 1.0;
	a1_check.style.zIndex = 100;
	

	if (Math.round((parseFloat(p_bb_form.elements[0].value) + 0.00001) * 100) / 100 == Math.round(((total_blue * 1.0 / (total_blue + total_gold)) + 0.00001) * 100) / 100) {
		document.getElementById("check_1_box").innerHTML = "Correct!";
	} else {
		document.getElementById("check_1_box").innerHTML = "Incorrect";
	}
}

function check_lik_wg() {
	a4_check.style.opacity = 1.0;
	a4_check.style.zIndex = 100;
	
	if (Math.round((parseFloat(lik_wg_form.elements[0].value) + 0.00001) * 100) / 100 == Math.round(((seconds_b * 1.0 / (seconds_y + seconds_b)) + 0.00001) * 100) / 100) {
		document.getElementById("check_4_box").innerHTML = "Correct!";
	} else {
		document.getElementById("check_4_box").innerHTML = "Incorrect";
	}
}


function check_lik_bb() {
	a3_check.style.opacity = 1.0;
	a3_check.style.zIndex = 100;
	
	if (Math.round((parseFloat(lik_bb_form.elements[0].value) + 0.00001) * 100) / 100 == Math.round(((seconds_y * 1.0 / (seconds_y + seconds_b)) + 0.00001) * 100) / 100) {
		document.getElementById("check_3_box").innerHTML = "Correct!";
	} else {
		document.getElementById("check_3_box").innerHTML = "Incorrect";
	}

}


function check_z() {
	a5_check.style.opacity = 1.0;
	a5_check.style.zIndex = 100;
	
	if (Math.round((parseFloat(z_form.elements[0].value) + 0.00001) * 100) / 100 == Math.round(((total_blue * seconds_y * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))  + total_gold * seconds_b * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))     ) + 0.00001) * 100) / 100) {
		document.getElementById("check_5_box").innerHTML = "Correct!";
	} else {
		document.getElementById("check_5_box").innerHTML = "Incorrect";
	}
}

function check_all() {
	var wrong_list = [];

	all_checked.style.opacity = 1.0;
	all_checked.style.zIndex = 100;


	if (Math.round((parseFloat(p_bb_form.elements[0].value) + 0.00001) * 100) / 100 == Math.round(((total_blue * 1.0 / (total_blue + total_gold)) + 0.00001) * 100) / 100) {
	} else {
		wrong_list.push("p(black and blue)");
	}




	if (Math.round((parseFloat(p_wg_form.elements[0].value) + 0.00001) * 100) / 100 == Math.round(((total_gold * 1.0 / (total_blue + total_gold)) + 0.00001) * 100) / 100) {
	} else {
		wrong_list.push("p(white and gold)");
	}

	if (Math.round((parseFloat(lik_wg_form.elements[0].value) + 0.00001) * 100) / 100 == Math.round(((seconds_b * 1.0 / (seconds_y + seconds_b)) + 0.00001) * 100) / 100) {
	} else {
		wrong_list.push("p(image | black and blue)");
	}

	if (Math.round((parseFloat(lik_bb_form.elements[0].value) + 0.00001) * 100) / 100 == Math.round(((seconds_y * 1.0 / (seconds_y + seconds_b)) + 0.00001) * 100) / 100) {
	} else {
		wrong_list.push("p(image | white and gold)");
	}

	if (Math.round((parseFloat(z_form.elements[0].value) + 0.00001) * 100) / 100 == Math.round(((total_blue * seconds_y * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))  + total_gold * seconds_b * 1.0 / ((seconds_y + seconds_b) * (total_blue + total_gold))     ) + 0.00001) * 100) / 100) {
	} else {
		wrong_list.push("p(image)");
	}

	if (wrong_list.length == 0) {
		document.getElementById("check_results").innerHTML = "All values are correct! Click next to proceed.";
		next6.disabled = false;
	} else {
		document.getElementById("check_results").innerHTML = "The following entities have incorrect values (correct them to proceed):" + wrong_list.join();
	}
}



