	var octopus = octopus1.concat(octopus2, octopus3);
	var clam = clam1.concat(clam2, clam3);

	var mammal = wolf.concat(rhino, horse);
	var bird = roadrunner.concat(owl, goldfinch, raven);
	var fish = clownfish.concat(shark, tuna);
	var reptile = snake.concat(tortoise);
	var amphibian = frog;
	var arthropod = spider.concat(ant, beetle, grasshopper);
	var mollusc = octopus.concat(clam);

	var vertebrate = mammal.concat(bird, fish, reptile, amphibian);
	var invertebrate = arthropod.concat(mollusc);

	var animal = vertebrate.concat(invertebrate);


	var implant = animal;
	var noninvertible = vertebrate;
	var spineless = invertebrate;
	var gotmilk = mammal;
	var wingbearer = bird;
