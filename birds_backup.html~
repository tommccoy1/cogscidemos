//}


var last_seen = 0;

function show_next_panel() {
	last_seen++;
	document.getElementById("image_" + last_seen).style.opacity = 1;
	if (last_seen > 1) {
	document.getElementById("possibility_" + last_seen).style.opacity = 1;

	} else {
		poss.style.opacity = 1;
		prior.style.opacity = 0;
		lik.style.opacity = 0;
		post.style.opacity = 0;
	}	
}


var posts = [[0.1, 1], [0.12, 2], [0.02, 3], [0.0011, 4], [0.001, 5]];
function put_in_order() {

	function callback(a){
		return function(){
			document.getElementById(a).style.opacity = 1.0;
		}
	}

	var name1 = "prior";
	setTimeout(callback(name1),500);
	name1 = "lik";
	setTimeout(callback(name1), 2000);
	name2 = "post";
	setTimeout(callback(name2), 4000);

	var new_posts = posts.sort(function(a,b){return b[0] - a[0];});

	var i = -1;
	var prev_num = -1;

	function theLoop (j) {
		setTimeout(function () {

		document.getElementById("poss_" + new_posts[i][1]).style.opacity = 0.0;
		document.getElementById("prior_" + new_posts[i][1]).style.opacity = 0.0;
		document.getElementById("lik_" + new_posts[i][1]).style.opacity = 0.0;
		document.getElementById("post_" + new_posts[i][1]).style.opacity = 0.0;
		
		document.getElementById("pos_1_" + (i+1)).style.opacity = 1.0;

		if (--j) {
			theLoop(j);
		} 

		}, 3000);	
		document.getElementById("poss_" + new_posts[i+1][1]).style.color = "red";
		document.getElementById("prior_" + new_posts[i+1][1]).style.color = "red";
		document.getElementById("lik_" + new_posts[i+1][1]).style.color = "red";
		document.getElementById("post_" + new_posts[i+1][1]).style.color = "red";




		i++;
	}

	function callback_2() {
		return function(){
			theLoop(5);
		}
	}

	setTimeout(callback_2(), 8000);


}

